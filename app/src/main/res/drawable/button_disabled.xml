<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" >
        <shape>
            <gradient
                android:endColor="#D9EDEB"
                android:startColor="#BEDDDF"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#94C4C4" />
            <corners
                android:radius="3dp" />
            <padding
                android:left="18dp"
                android:top="13dp"
                android:right="18dp"
                android:bottom="13dp" />
        </shape>
    </item>
</selector>